    "from pytube import YouTube\n",
    "url = input(\"please enter the link of YouTube video : \")\n",
    "yt = YouTube (url)\n",
    "ys = yt.streams.get_highest_resolution()\n",
    "\n",
    "print(\"Downloading the video , please wait...\")\n",
    "ys.download()\n",
    "print(\"Finished downloading!\")"
   ]
